pipeline {
  
  agent any 
  stages {
    stage("Build"){
      steps{
        // here you will put the git hub repoitory url
          sh 'echo "Installgin the dependencies"'
      }
    }
    stage("deploy"){
      steps{
          sh 'echo "deploying be done"'
      }
    }
    stage("test"){
      steps{
          sh 'echo "test the application is working or not"'
      }
    }
    stage("validate"){
      steps{
          sh 'echo "It is validating the dependencies"'
      }
    }
  }
}
